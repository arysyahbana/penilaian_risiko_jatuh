{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.3"
        }
    },
    "routes": [
        {
            "src": "/assets/img/(.*)",
            "dest": "/public/build/assets/img/$1"
        },
        {
            "src": "/assets/css/(.*)",
            "dest": "/public/build/assets/css/$1"
        },
        {
            "src": "/assets/fonts/(.*)",
            "dest": "/public/build/assets/fonts/$1"
        },
        {
            "src": "/assets/js/(.*)",
            "dest": "/public/build/assets/js/$1"
        },
        {
            "src": "/assets/scss/(.*)",
            "dest": "/public/build/assets/scss/$1"
        },
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/build/assets/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://penilaian-risiko-jatuh-three.vercel.app/",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    },
    "outputDirectory": "public"
}
